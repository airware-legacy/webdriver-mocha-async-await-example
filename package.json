{
  "name": "webdriver-mocha-async-await-example",
  "version": "1.0.0",
  "description": "Using Mocha, Selenium Webdriver, and Babel together to provide async / await to browser e2e tests",
  "scripts": {
    "test": "./node_modules/.bin/_mocha",
    "install": "./node_modules/.bin/selenium-standalone install",
    "start": "./node_modules/.bin/selenium-standalone start",
    "lint": "./node_modules/.bin/eslint .",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airware/webdriver-mocha-async-await-example.git"
  },
  "author": "Airware",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airware/webdriver-mocha-async-await-example/issues"
  },
  "homepage": "https://github.com/airware/webdriver-mocha-async-await-example#readme",
  "dependencies": {
    "babel-preset-env": "^1.1.10",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "mocha": "^3.1.2",
    "selenium-standalone": "^5.7.2",
    "selenium-webdriver": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.16.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-standard": "^2.0.1",
    "flow-bin": "^0.40.0"
  }
}
